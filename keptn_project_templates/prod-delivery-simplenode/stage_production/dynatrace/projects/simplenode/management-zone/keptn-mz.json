              },
              "negate": false
            }
          },
          {
            "key": {
              "attribute": "SERVICE_TAGS"
            },
            "comparisonInfo": {
              "type": "TAG",
              "operator": "EQUALS",
              "value": {
                "context": "ENVIRONMENT",
                "key": "DT_RELEASE_STAGE",
                "value": "{{ .stage }}"
              },
              "negate": false
            }
          }
        ]
      }
    ],
    "dimensionalRules": []
